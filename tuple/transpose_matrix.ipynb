    "# Example1 transpose a matrix\n",
    "X = [[12,9],\n",
    "     [7,3],\n",
    "     [5,6]]\n",
    "result =[[0,0,0],\n",
    "         [0,0,0]]\n",
    "# iterate through rows\n",
    "for i in range(len(X)):\n",
    "    #iterate through columns\n",
    "    for j in range(len(X[0])):\n",
    "        result[j][i] = X[i][j]\n",
    "for i in result:\n",
    "    print(i)"
